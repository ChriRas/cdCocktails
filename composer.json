{
  "name": "chriras/cd-cocktail",
  "type": "project",
  "description": "A simple cocktail menu",
  "license": "MIT",
  "authors": [
    {
      "name": "Christoph Raschke",
      "homepage": "https://github.com/chriras",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/yaml": "^7.0",
    "nesbot/carbon": "^3.0",
    "vlucas/phpdotenv": "^5.6",
    "symfony/console": "^7.0",
    "ext-imagick": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^11",
    "phpstan/phpstan": "^2"
  },
  "scripts": {
    "test": "phpunit --testdox",
    "test:unit": "phpunit",
    "stan": "phpstan analyse --memory-limit=512M"
  }
}
